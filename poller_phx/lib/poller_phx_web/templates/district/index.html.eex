<h1>Districts</h1>

<table>
    <thead>
        <tr>
            <th colspan="2">
                Name
            </th>
        </tr>
    </thead>
    <tbody>
        <%= for district <- @districts do %>
            <tr>
                <td><%= district.name %></td>
                <td>...</td>
            </tr>
        <% end %>
    </tbody>
</table
